# -*- coding: utf-8 -*-
# UTF-8 encoding when using korean
n, m = map(int, input().split())
state = list(map(int, input().split()))
rec = set()

for i in range(1, m+1):
	s, e = map(int, input().split())

	for j in range(s-1, e):
		state[j] += 1
		rec.add(j)
		
	if i % 3 == 0:
		for j in range(n):
			if j in rec:
				state[j] -= 1
				rec.remove(j)

print(" ".join(map(str, state)))
